<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="p">

<select id="pay_info" resultType="pay">
	SELECT * FROM ITEM
</select> 
	
	<update id="paying_update" parameterType="String">
		UPDATE ITEM SET PAYMENT=#{payment}
	</update>
</mapper>












